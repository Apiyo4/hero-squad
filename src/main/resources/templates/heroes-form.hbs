{{#partial "content"}}
    <div class=" marginTop">
    <h1>Add a new Squad</h1>
    <form action="/heroes/list" method="post">
        <div class="form-group">
            <label for="name"> Name </label>
            <input id="name" type="text" class="form-control" name="name">
        </div>
        <div class="form-group">
            <label for="age">Age</label>
            <input id="age" type="text" class="form-control" name="age">
        </div>
        <div class="form-group">
            <label for="specialPower">SpecialPower</label>
            <input id="specialPower" type="text" class="form-control" name="specialPower">
        </div>
        <div class="form-group">
            <label for="weakness">Weakness</label>
            <input id="weakness" type="text" class="form-control" name="weakness">
        </div>

        <button type="submit" class="btn btn-success">Submit</button>
    </form>
    <h5> To go back</h5>
    </div>
{{/partial}}
{{> layout.hbs}}

